<% if (!user) { %>
	<p>Welcome! Please <a href="http://localhost:3001/auth/google">log in</a>.</p>
<% } else { %>
    <p>Hello, <%= user.id%></p>
    <p>Hello, <%= user.displayName%></p>
    <a href="/logout">logout</a>
    <br>
    <img src="<%= user._json.picture%>" alt="foto red social"> 
<% } %>



